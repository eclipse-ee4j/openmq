    <scm>
        <connection>scm:git:https://github.com/eclipse-ee4j/openmq.git</connection>
        <developerConnection>scm:git:git@github.com:eclipse-ee4j/openmq.git</developerConnection>
        <url>https://github.com/eclipse-ee4j/openmq</url>
    </scm>

    <properties>
        <maven.compiler.release>11</maven.compiler.release>
        <maven.compiler.testRelease>17</maven.compiler.testRelease>
        <!-- Change these for each build -->
        <build.number>1</build.number>
        <build.letter>a</build.letter>

        <!-- Product-specific -->
        <full.name>Eclipse OpenMQ(tm)</full.name>
        <brand.name>Eclipse OpenMQ(tm)</brand.name>
        <vendor.name>Eclipse Foundation</vendor.name>
        <product.name>Message Queue</product.name>
        <product.abbrevname>MQ</product.abbrevname>
        <product.copyright>Copyright (c) 2012, 2018 Oracle and/or its affiliates. All rights reserved.
                           Copyright (c) 2018, 2022 Contributors to Eclipse Foundation.</product.copyright>

        <!-- Versions of dependencies -->
        <hk2.version>3.0.3</hk2.version>
        <jakarta-jms.version>3.1.0</jakarta-jms.version>

        <grizzly.version>4.0.0</grizzly.version>

        <junit.version>5.8.2</junit.version>
        <assertj.version>3.23.1</assertj.version>
        <mockito.version>4.6.1</mockito.version>
        <lombok.version>1.18.24</lombok.version>
        <pmd.version>6.47.0</pmd.version>
        <pmd.plugin.version>3.17.0</pmd.plugin.version>
        <spotbugs.version>4.7.1</spotbugs.version>
        <spotbugs.plugin.version>4.7.1.0</spotbugs.plugin.version>
        <checkstyle.version>10.3.1</checkstyle.version>
        <maven.checkstyle.plugin.version>3.1.2</maven.checkstyle.plugin.version>
    </properties>

